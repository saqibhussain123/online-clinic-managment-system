<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="stylesheet" href="~/Content/assets/css/maicons.css">

    <link rel="stylesheet" href="~/Content/assets/css/bootstrap.css">

    <link rel="stylesheet" href="~/Content/assets/vendor/owl-carousel/css/owl.carousel.css">

    <link rel="stylesheet" href="~/Content/assets/vendor/animate/animate.css">

    <link rel="stylesheet" href="~/Content/assets/css/theme.css">

</head>
<body>
    <div class="back-to-top"></div>

    <header>
        <div class="topbar">
            <div class="container">
                <div class="row">
                    <div class="col-sm-8 text-sm">
                        <div class="site-info">
                            <a href="#"><span class="mai-call text-primary"></span> +923028718494</a>
                            <span class="divider">|</span>
                            <a href="#"><span class="mai-mail text-primary"></span> saqibhussains930@gmail.com</a>
                        </div>
                    </div>
                    <div class="col-sm-4 text-right text-sm">
                        <div class="social-mini-button">
                            <a href="#"><span class="mai-logo-facebook-f"></span></a>
                            <a href="#"><span class="mai-logo-twitter"></span></a>
                            <a href="#"><span class="mai-logo-dribbble"></span></a>
                            <a href="#"><span class="mai-logo-instagram"></span></a>
                        </div>
                    </div>
                </div> <!-- .row -->
            </div> <!-- .container -->
        </div> <!-- .topbar -->

        <nav class="navbar navbar-expand-lg navbar-light shadow-sm">
            <div class="container">
                <a class="navbar-brand" href="~/Home/Index"><span class="text-primary">Clinic</span>-Managment system</a>



                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupport" aria-controls="navbarSupport" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupport">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item active">
                            <a class="nav-link" href="~/Home/Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="~/Home/About">About Us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="~/Home/doctors">Doctors</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="~/Home/blog">News</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="~/Home/lab">Lab</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="~/Home/pharmacy">Pharmacy</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="~/Home/Education">Education</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="~/Home/bussines">Bussines</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="~/Home/Contact">Contact</a>
                        </li>
                       <li>
                           @Html.Partial("_LoginPartial")
                       </li>
                    </ul>
                </div> <!-- .navbar-collapse -->
            </div> <!-- .container -->
        </nav>
    </header>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer class="page-footer">
            <div class="container">
                <div class="row px-md-3">
                    <div class="col-sm-6 col-lg-3 py-3">
                        <h5>Company</h5>
                        <ul class="footer-menu">
                            <li><a href="~/Home/About">About Us</a></li>
                            <li><a href="~/Home/Education">Career</a></li>
                            <li><a href="~/Home/bussines">Bussiness</a></li>
                            <li><a href="~/Home/blog">Protection</a></li>
                        </ul>
                    </div>
                    <div class="col-sm-6 col-lg-3 py-3">
                        <h5>More</h5>
                        <ul class="footer-menu">
                            <li><a href="~/Home/About">Terms & Condition</a></li>
                            <li><a href="~/Home/Contact">Privacy</a></li>
                            <li><a href="#">Advertise</a></li>
                            <li><a href="~/Home/doctors">Join as Doctors</a></li>
                        </ul>
                    </div>
                    <div class="col-sm-6 col-lg-3 py-3">
                        <h5>Our partner</h5>
                        <ul class="footer-menu">
                            <li><a href="#">One-Fitness</a></li>
                            <li><a href="#">One-Drugs</a></li>
                            <li><a href="#">One-Live</a></li>
                        </ul>
                    </div>
                    <div class="col-sm-6 col-lg-3 py-3">
                        <h5>Contact</h5>
                        <p class="footer-link mt-2">Gulshan johar karachi pakistan</p>
                        <a href="#" class="footer-link">Phone</a>
                        <a href="saqibhussains930@gmail.com" class="footer-link">Email</a>

                        <h5 class="mt-3">Social Media</h5>
                        <div class="footer-sosmed mt-3">
                            <a href="#" target="_blank"><span class="mai-logo-facebook-f"></span></a>
                            <a href="#" target="_blank"><span class="mai-logo-twitter"></span></a>
                            <a href="#" target="_blank"><span class="mai-logo-google-plus-g"></span></a>
                            <a href="#" target="_blank"><span class="mai-logo-instagram"></span></a>
                            <a href="#" target="_blank"><span class="mai-logo-linkedin"></span></a>
                        </div>
                    </div>
                </div>

               
            </div>
        </footer>

        <script src="~/Content/assets/js/jquery-3.5.1.min.js"></script>

        <script src="~/Content/assets/js/bootstrap.bundle.min.js"></script>

        <script src="~/Content/assets/vendor/owl-carousel/js/owl.carousel.min.js"></script>

        <script src="~/Content/assets/vendor/wow/wow.min.js"></script>

        <script src="~/Content/assets/js/theme.js"></script>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
